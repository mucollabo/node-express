<div class="weatherWiget">
    {{#each partials.weatherContext}}
        <div class="location">
            <h3>{{location.name}}</h3>
            <a href="{{location.forecastUrl}}">
                <img src="{{iconUrl}}" alt="{{weather}}">
                {{weather}}, {{temp}}
            </a>
        </div>
    {{/each}}
    <small>Source: <a href="https://www.weather.gov/documentation/service-web-api"></a></small>
</div>
